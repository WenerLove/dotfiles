[push]
	default = simple
[user]
	editor = gvim
	quotepath = false
	name = wener
	email = wenermail@gmail.com
[alias]
	ci = commit
	co = checkout
	loggo = log --graph --oneline
	loggod = log --graph --oneline --decorate
	logg = log --graph
	sti = status --ignored
	st = status
	sts = status -s
    stsb = status -sb
    latest = clone --depth=1
[color]
    diff = auto
    status = auto
    branch = auto
	ui = true
[core]
	autocrlf = input
	safecrlf = false
	quotepath = off
[credential]
	helper = cache --timeout=3600
[url "https://"]
	insteadOf = git://

