[push]
  default = simple
[user]
	editor = nano
	quotepath = false
	name = wener
	email = wenermail@gmail.com
[alias]
	master = checkout master
	ci = commit
	co = checkout
	wt = worktree
	loggo = log --graph --oneline
	l = log --graph --oneline --decorate
	logg = log --graph
	sti = status --ignored
	st = status
	sts = status -s
  stsb = status -sb
  latest = clone --depth 1 -v --progress
	# svn push
	svnc = !git stash && git svn dcommit && git stash pop
	svnr = !git stash && git svn rebase  && git stash pop
[color]
    diff = auto
    status = auto
    branch = auto
	ui = true
[core]
	autocrlf = input
	safecrlf = false
	quotepath = false
	ignorecase = false
[credential]
	helper = cache --timeout=36000
